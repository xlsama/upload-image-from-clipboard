{
  "name": "upload-image-from-clipboard",
  "version": "0.0.6",
  "description": "Upload image from clipboard to SM.MS.",
  "license": "MIT",
  "author": "xlsama",
  "main": "./dist/index.cjs",
  "module": "dist/index.mjs",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist",
    "lib"
  ],
  "bin": {
    "ui": "dist/index.mjs"
  },
  "repository": "xlsama/upload-image-from-clipboard",
  "homepage": "https://github.com/xlsama/upload-image-from-clipboard",
  "scripts": {
    "dev": "esno src/index.ts",
    "build": "unbuild && cp -r src/lib dist",
    "release": "npm run build && npm publish"
  },
  "keywords": [
    "image",
    "clipboard"
  ],
  "dependencies": {
    "commander": "^9.4.1",
    "form-data": "^4.0.0",
    "node-fetch": "^3.3.0"
  },
  "devDependencies": {
    "@types/node": "^18.11.9",
    "esno": "^0.16.3",
    "typescript": "^4.9.3",
    "unbuild": "^1.0.1"
  }
}
